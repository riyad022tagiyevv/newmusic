{
  "name": "Rahid Music 🎸",
  "description": "Telegram qrup səs çatında mahnıları səsləndirmək üçün açıq mənbə botu. Powered by Rahid.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/riyad022tagiyevv/newmusic",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session'u yerləşdirin (Replit.com)",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Bot token telegram @botfather-dan alın",
      "required": true
    },
    "BOT_NAME": {
      "description": "Musiqi botun adı yazın.",
      "required": false,
      "value": ""
    },
    "BOT_USARNAME": {
      "description": "Musiqi botun istifadəçi adı @ olmadan yazın.",
      "required": true,
      "value": ""
     
    },
    "API_ID": {
      "description": "my.telegram saytda olan API-ID'ni yerləşdirin",
      "required": true
    },
    "API_HASH": {
      "description": "my.telegram saytda olan API-HASH yerləşdirin",
      "required": true
    },
    "SUDO_USERS": {
      "description": "Hər yerdə idarəçi sayılan istifadəçi ID-lərinin siyahısı (Musiqi asistan hesabının ID'si).",
      "required": true
    },
    "BOT_OWNER": {
      "description": "Öz tag adını yaz",
      "required": true,
      "value": "BrendUIvi"
    },
    "DURATION_LIMIT": {
      "description": "Yükləmələr üçün maksimum audio vaxt limiti (dəqiqə).",
      "required": true,
      "value": "55"
    }
  }
}
